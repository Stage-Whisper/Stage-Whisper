{
  "name": "stagewhisper",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "null"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "build": "yarn build:main && yarn build:preload && yarn build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "cross-env MODE=production yarn build && electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "test": "yarn test:main && yarn test:preload && yarn test:renderer && yarn test:e2e",
    "test:e2e": "yarn build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "watch": "node scripts/watch.mjs",
    "lint": "eslint . --ext js,mjs,cjs,ts,mts,cts,tsx",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "yarn typecheck:main && yarn typecheck:preload && yarn typecheck:renderer",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.mjs",
    "format": "npx prettier --write \"**/*.{js,mjs,cjs,ts,mts,cts,tsx,json}\""
  },
  "devDependencies": {
    "@types/eslint": "8.4.10",
    "@types/howler": "^2.2.7",
    "@types/node": "^18.11.15",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-vue": "4.0.0",
    "@vue/test-utils": "2.2.6",
    "cross-env": "7.0.3",
    "electron": "22.0.0",
    "electron-builder": "23.6.0",
    "eslint": "8.29.0",
    "eslint-plugin-vue": "9.8.0",
    "happy-dom": "8.1.0",
    "lint-staged": "^13.1.0",
    "nano-staged": "0.8.0",
    "playwright": "1.27.1",
    "simple-git-hooks": "2.8.1",
    "typescript": "4.9.4",
    "unplugin-auto-expose": "0.0.4",
    "vite": "4.0.1",
    "vitest": "0.25.8",
    "vue-tsc": "1.0.13"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@mantine/core": "^5.9.4",
    "@mantine/dates": "^5.9.4",
    "@mantine/dropzone": "^5.9.4",
    "@mantine/form": "^5.9.4",
    "@mantine/hooks": "^5.9.4",
    "@mantine/modals": "^5.9.4",
    "@mantine/notifications": "^5.9.4",
    "@mantine/nprogress": "^5.9.4",
    "@mantine/rte": "^5.9.4",
    "@reduxjs/toolkit": "^1.9.1",
    "@tabler/icons": "^1.117.0",
    "@vitejs/plugin-react": "^3.0.0",
    "dayjs": "^1.11.7",
    "electron-router-dom": "^1.0.5",
    "electron-updater": "5.3.0",
    "eslint-plugin-react": "^7.31.11",
    "howler": "^2.2.3",
    "knex": "^2.3.0",
    "lorem-ipsum": "^2.0.8",
    "mantine-datatable": "^1.7.25",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-localization": "^1.0.19",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.5",
    "sqlite3": "^5.1.4",
    "subtitle": "^4.2.1",
    "uuid": "^9.0.0",
    "vue": "3.2.45"
  }
}
