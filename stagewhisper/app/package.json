{
  "name": "stage-whisper-electron",
  "version": "0.1",
  "license": "MIT",
  "main": "main/index.js",
  "author": {
    "name": "null"
  },
  "description": "Electron app with React, TypeScript and Vite for the Stage-Whisper project",
  "keywords": [
    "vite",
    "react",
    "electron",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/Stage-Whisper/Stage-Whisper"
  },
  "scripts": {
    "dev": "concurrently \"yarn dev:vite\" \" yarn dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron && electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@mantine/core": "^5.4.1",
    "@mantine/dates": "^5.4.1",
    "@mantine/dropzone": "^5.4.1",
    "@mantine/form": "^5.4.1",
    "@mantine/hooks": "^5.4.1",
    "@mantine/modals": "^5.4.1",
    "@mantine/notifications": "^5.4.1",
    "@mantine/nprogress": "^5.4.1",
    "@mantine/rte": "^5.4.1",
    "@tabler/icons": "^1.101.0",
    "@vitejs/plugin-react": "^1.2.0",
    "dayjs": "^1.11.5",
    "electron-is-dev": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.4.1",
    "vite": "^2.8.6"
  },
  "devDependencies": {
    "@types/eslint": "8.4.6",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/node": "^17.0.22",
    "@types/prettier": "2.6.0",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.0.0",
    "electron": "^17.1.2",
    "electron-builder": "^22.14.13",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "prettier": "2.6.0",
    "typescript": "^4.6.2"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "src/out"
    ],
    "directories": {
      "buildResources": "resources"
    }
  }
}
